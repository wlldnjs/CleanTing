<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sopt.client.cleanting">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Application.ApplicationController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".Main.MainActivity" />
        <activity android:name=".Main.Login.LoginActivity">

        </activity>
        <activity android:name=".Main.SignUp.SignUpActivity" />
        <activity android:name=".Main.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>